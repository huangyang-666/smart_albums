<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huang.mapper.othermapper">
    <select id="addotherInfo">
        insert into others(`imgName`, `uploadTime`, `imgSize`, `imgType`, `details`, `imgPath`, `master`, `md5`,`name`)
        VALUES (#{imgName}, #{uploadTime}, #{imgSize}, #{imgType}, #{details}, #{imgPath}, #{master}, #{md5},#{name})
    </select>
    <!--    查询所有照片-->
    <select id="queryothers" resultType="other">
        select *
        from others
        where `master` = #{master}
    </select>

    <delete id="delothertimg">
        delete from others where `master`=#{master} and `name`=#{name}

    </delete>


</mapper>