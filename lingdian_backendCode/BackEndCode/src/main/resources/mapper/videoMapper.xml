<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huang.mapper.videomapper">
    <select id="addvideoInfo">
            insert into video ('uploadTime','videoName','videoPath','master')
             VALUES (#{uploadTime},#{videoName},#{videoPath},#{master})
    </select>
    <select id="queryvideo" resultType="video">
        select *
        from video
        where `master` = #{master}
    </select>
</mapper>